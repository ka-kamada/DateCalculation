<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- Mapperとxmlのマッピング -->
<mapper namespace="com.example.repository.UserMapper">

<!-- ユーザー登録 -->
	<insert id="setUser">
    insert into user(
        user_id
      , password
      , user_name
    )
    values (
        #{userId}
      , #{password}
      , #{userName}
    )
  </insert>



</mapper>
